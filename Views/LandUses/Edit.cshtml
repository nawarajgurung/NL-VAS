@model NL_VAS.Entity.LandUse

@{
    ViewBag.Title = "Edit";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-12">
            <div class="card-box">
                <h2>Edit</h2>
                <div class="row">
                    <div class="col-12">
                        <div class="p-20">
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            @Html.HiddenFor(model => model.Id)
                            <form class="form-horizontal" role="form">
                                <div class="form-group row">
                                    <label class="col-2 col-form-label">

                                        @Html.LabelFor(model => model.GeneralInfoParcelId, "GeneralInfoParcelId", htmlAttributes: new { @class = "control-label col-md-2" })                                    </label>
                                    <div class="col-10">
                                        @Html.DropDownList("GeneralInfoParcelId", null, htmlAttributes: new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.GeneralInfoParcelId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-2 col-form-label">
                                        @Html.LabelFor(model => model.LandTypeId, "LandTypeId", htmlAttributes: new { @class = "control-label col-md-2" })
                                    </label>
                                    <div class="col-10">
                                        @Html.DropDownList("LandTypeId", null, htmlAttributes: new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.LandTypeId, "", new { @class = "text-danger" })
                                    </div>

                            </form>
                        </div>

                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-12">
                                <input type="submit" value="Save" class="btn btn-default pull-right" />
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- end row -->

        </div> <!-- end card-box -->
    </div><!-- end col -->

}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
